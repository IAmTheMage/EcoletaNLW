    "@types/multer": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "knex": "^2.4.0",
    "multer": "^1.4.2",
    "pg": "^8.2.1",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.44"
  }
